{
    "name": "inpsyde/wp-stash",
    "description": "Powerful Object Caching Backend for WordPress",
    "license": "GPL-2.0+",
    "type": "wordpress-muplugin",
    "authors": [
        {
            "name": "Biont",
            "email": "m.meisselbach@inpsyde.com"
        },
        {
            "name": "ChriCo",
